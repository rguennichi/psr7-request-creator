{
    "name": "guennichi/psr7-request-factory",
    "description": "Factory class to create request body from PSR-7 server requests",
    "license": "MIT",
    "keywords": ["psr-7", "psr-17"],
    "homepage": "https://guennichi.com",
    "authors": [
        {
            "name": "Radhi Guennichi",
            "email": "hello@guennichi.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-message": "^1.0",
        "php-http/multipart-stream-builder": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpstan/phpstan": "^1.8",
        "nyholm/psr7": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Guennichi\\Psr7RequestFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Guennichi\\Psr7RequestFactory\\": "tests/"
        }
    }
}
